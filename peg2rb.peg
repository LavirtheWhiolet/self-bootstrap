class Code
  
  def initialize(parts)
    @parts = parts
  end
  
  def self.from x
    case x
    when Code then x
    when String then Code.new([x])
    else raise %(can not convert #{x.inspect} to #{Code})
    end
  end
  
  protected
  
  # Private.
  attr_reader :parts
  
end


